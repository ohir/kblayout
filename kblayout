#!/usr/bin/perl -w -s
# Sane XKB configuration tool.
# (c)2020 OHIR RIPE. Dual licensed under MIT and GPLv3.
#
# This script is meant to be used once a decade or so. It does use globals,
# copy code, mutate global data in subs, and it keeps flow mostly linear.
# I do not see merit in engineering this script to be easy maintainable as
# XKB itself is so stable that its maintainers still refuse to merge
# spec-fixing patches used by downstream for decades now ().
# TODO
# [x] list current layout to the terminal
# [ ] prepare (init) sane maps using current layout
# [ ] generate XKB files from the maps
use v5.26.1; ### UCD v6. There is no sense in using older perls for this script.
use utf8; use integer;
# check unicode status
unless($ENV{PERL_UNICODE} and $ENV{PERL_UNICODE}=~/^(?:[SDA]{3}|IOEioA)$/){
die<<DOE
Unicode environment is not properly configured.
run as: PERL_UNICODE=SDA $0 [options]
        Alternatively add below line to your .bashrc or .zshrc:
 export PERL_UNICODE=SDA
DOE
}
### Bad (obsolete) ucd corrections:  OS/version dependent!
my $W="ğŸ¦“";   # put your out-of-ucd table wide glyphs here
my $O="â‡â˜â˜â˜"; # put your right-overflowing glyphs here. Overflow happens
              # when ucd used by display device has got width wrong.

### default dirs and files
our $xkbdir or $xkbdir = '/usr/share/X11/xkb';
our $cnfdir or $cnfdir = "$ENV{HOME}/.xkb/sane";
our $xkdata or $xkdata ='./xkbs-data';# testing, hacking
sub usage { die shift||<<DOE
  Usage: $0 [options] command arg, arg file[.skd]

    commands:
      -show      print layout for gears: 1/2 3/4 5/6
                 Default if command runs as "kblayout"
      -mknew [=filename.skd] ### not implemented yet ##
                 make stub layout file in current empty directory

    options:
      -h        this help
      -v        be verbose
      -g7       show for gears: 1/2 7/8 9/10
      -num      show edit and numeric block layout 
      -ansi     use ansi terminal codes for output (dead keys mostly)
      -wide=ch  Correct display for a wide character(s) given.
      -ovfl=ch  Correct display for a overflowing character(s) given.

      ########### Generators are not implemented yet! #######################
      -legacy   generate for systems with none or poor UTF8 locale support
                This will produce tables using latinX keysyms (where normally
                sane tables would use utf for all but ascii symbols).
                Note that you can not edit .xkbs source on legacy systems,
                as old "codepage/latinX" symbols in the source would not
                be properly recognized.

      -xkbdir=$xkbdir # xkb system dir
      -cnfdir=$cnfdir # user definitions dir
DOE
;}
our $ovfl; $ovfl and $ovfl=~/[^A-Za-z0-9]+/ and $O.=$ovfl, say "Overflows: $O";
our $wide; $wide and $wide=~/[^A-Za-z0-9]+/ and $W.=$wide, say "Wide: $W";
# below opts for someone who knows what she's doing
#    -xkdata           // dev data, keysyms and other constant stuff
#    -pcgeom=pc105     // keyboard type: pc104|pc105
#    -driver=evdev     // some still use xfree86
#    -extras=inet      // xkb default for evdev driver
#    list              // command: flat
our $h and usage; our ($v); # help, verbose
our $driver or $driver = 'evdev';     # some still use xfree86
our $pcgeom or $pcgeom = 'pc105';     # default is the best
our $extras or $extras = 'inet';      # media keys etc
our $ansi;  my ($invb,$inve)=("\x1b[7m","\x1b[0m");
our $g7;    my @SG=$g7?(1,2,7,8,9,10):(1,2,3,4,5,6);
our($list, $show, $num);
my (%def, %outstr, %byU, $fh, $footer); $footer='';
my $legendtab=10;  # console output
my $legendmax=115; # console min width is 116 anyway

sub initmaps; sub rdata; sub repr; sub alnum;
sub showcurr; sub listwhat;

### main ###
initmaps;
if($0=~/kblayout$/ or $show){ # print layout to the terminal
    my $w=$num?'consnum':'console'; print alnum($w),legend();
    $footer and print $footer;
    exit 0;
}
$list and listwhat($list), exit 1;    # Print linear list (layout debug helper).

### subs
sub alnum { my ($g,$r,$i)=@{$outstr{$_[0]}};
    return $def{$g}=~s/$i/$r->(\%+)/mrego; }
sub legend { my $lead; my $max=$legendtab;
  for(qw/0 1/){ my $i=-1; my $ll=0; my $p=$_;
  for(@{$def{legend}}) { $i++; $_ or next;
    my $g=$_; my $ou="G$i:"; $lead++ or print
    "-----Â¦  Short descriptions:  Â¦".('-' x ($legendmax-30))."\n";
    for(@{$g}){
      $ll>$legendmax-$max and $ll=0,$ou.="\n   ";
      my $u="  Â¦$_"; my $ul=length($u);
      $p or $max=($max<$ul?$ul:$max),next;
      $u.=' ' x ($max-$ul); $ll+=length($u); $ou.=$u;
    } $p and print "$ou\n"; $ll=0;
  }}
}

# TODO transformations for .json .glade .html
sub termo { my $m=shift;
    my ($a,$r,$d,$t,$c,$sh,$o,@g)=
       ($m->{a},$m->{r},$m->{d},$m->{t},$m->{c},0,' ');
    my @R=@{$def{repr}{$c}}; @R or return $a; unshift @R, '!';
    $t=~/^[A-I]/ and $sh=1; $o=($d=~/[â”‚â”ƒ]/)?$d:' ';
    for(@SG){ $_%2==$sh and next; push @g,$_}
    if($c==65){ # space is special
      for(@g){
        my $x=$R[$_]||'    '; my $sl=length($x);
        $sl<4 and $x.=(' ' x (4-$sl)); $sl>4 and $x=substr($x,0,4);
        $o.=chr(0x24f4+$_)." $x  ";
      } chop($o);
      return $o;
    }# space
    if($t eq 'A'){ # gear (control) keys are special too
      my $x=$R[1]||'    '; my $sl=length($x); my $pl=$sl;
      for($x){ $sl-=()=/\p{Mn}|\p{Me}/g; $sl+=()=/\p{Dt=Wide}|\p{Ea=W}/g; }
      $sl<4 and $x.=(' ' x (4-$sl)); $sl>4 and $x=substr($x,0,4);
      return "$o$x ";
    }
    for(@g){
      my $g=$_; my $x=$R[$_]||'';
      $x=~s/\s+//gaa; my $l=length($x);
      $x or $o.='  ', next; my $i;
      for($x){
        # if($i=$def{custkeys}{$x}){ last } # forced representation by input symbol
        if($l==1){ # singles
          $g<3 and $x=~/^[0-9A-Z]/aa  and chop($o), $i="$x\x{20de}  ", last;
          $x=~/^\p{Dt=Wide}|\p{Ea=W}/ and chop($o), $i="$x ", last; # fallback;
          index($O,$x)>=0 and chop($o), $i="$x  ", last; # correct for obsolete ncb
          $i="$x ", last;
        }
        elsif($l==2){ $i="$x "; $x=~/\p{Mn}|\p{Me}/ or chop($o); last; } # doubles
        elsif($x=~/^%([nwuFsc.])(.+)/){ # repr that differs from input symbolic
          # %n:asis %w:wide %u:uwide %F:Fn %s:square %c:circle %.:dead
          $1 eq 'n' and           $i="$2 ", last;
          $1 eq 'w' and chop($o), $i="$2 ", last;
          $1 eq 'u' and chop($o), $i="$2  ", last;
          $1 eq 'F' and $g<3 and chop($o), chop($o), $i="F".chr(0x2487+$2)." ", last;
          $1 eq 'f' and $i=chr(0x2487+$2)."\x{1DEB} ", last; # Fx at higher gears
          $1 eq 's' and $i="$2\x{20de} ", last;
          $1 eq 'c' and $i="$2\x{20dd} ", last;
          $1 eq '.' and $ansi and $i="$invb$2$inve ", last;
          $1 eq '.' and $i="$2\x{20e0} ", last;
          $i="$1\x{20e0} "; # bad letter
        }
        # make verbose legend for any other. Use â‚€âƒ§ ... â‚˜âƒ§á·µ markers.
        my $leg=$def{legnum}{$g}||0;$def{legnum}{$g}=$leg+1;
        my $xt=$leg>22?chr(0x1de6+$leg/10):''; # 240 markers per gear - enough.
        $i=($leg>9?chr(0x2090+(12-$leg%13)):chr(0x2080+$leg%10))."\x{20e7}$xt ";
        my $xx=$x.((length($x)<5)?' ' x (4-length($x)):''); # inner justify
        push @{$def{legend}[$g]},"$i$xx  ".@{$def{name}{$c}}[$g-1];
      } # for $x
      $o.=$i;
    } # for @g
    chop($o);
    return $o;
}

# get current kb layout, init transformations dict
sub initmaps {
  rdata; if(open(my $fh, "<",$xkdata)){ rdata $fh } # prepare data
  my (undef, $x)=split / /,`stty size`; chomp($x);
  ($? or $x=~/[^0-9]/) and usage "Install stty first!\n";
  $x<$legendmax and $footer.="\nToo narrow terminal (<$legendmax). Pipe output to |less -S.\n";
  $x=`xmodmap -pk`; $? and usage "Install xmodmap first!\n";
  for(split /^/, $x) { chomp;
    s/^\s*(\d+)\s+// or next; my $kn=$1; /0x/ or next;
    my ($i, $v, @ko); my @kl=split;
    my $h = $def{'khex'}{$kn}||[];
    my $n = $def{'name'}{$kn}||[];
    my $r = $def{'repr'}{$kn}||[];
    for($i=0;$i<20;$i++){ $v=$kl[$i];
        $v or $v=$i%2?'  ':'0x1000';
        push @ko, $v=~s/[()]//gr;
    }
    $ko[0] eq $ko[4] and $ko[4]=$ko[6]='0x1000',$ko[5]=$ko[7]='  ';
    $i=0; splice @ko,16,4,splice @ko,4,4; # mod2 => g9
    for(@ko){ $i++%2 and push(@$n,$_), next; push(@$h,$_); push(@$r,repr($_)); }
    $def{'name'}{$kn}=$n; $def{'khex'}{$kn}=$h; $def{'repr'}{$kn}=$r;
  }
  my $termre=qr/(?<a>(?<r>(?<d>\S) +(?<t>[A-Ia-i])_{0,2}(?<c>\d{1,3})))/m;
  %outstr=( # keep all outstream transformations in one place
    'console',[ 'pc105', \&termo, $termre],
    'consnum',[ 'pcnum', \&termo, $termre],
    ); 
  $def{'repr'}{'999'}=[qw(â“µ â“¶ â“· â“¸ â“¹ â“º â“» â“¼ â“½ â“¾)];
}

sub listwhat {
    my $pr=shift||1; $pr eq 'flat' and $pr = 1;
    for($pr){
    /1/ and do {
      for (sort{$a<=>$b}(keys(%{$def{'name'}}))) {
        print "$_ name => " . join(', ',    @{$def{'name'}{$_}})."\n";
        print "$_ khex => " . join(', ',    @{$def{'khex'}{$_}})."\n";
        print "$_ repr => â”‚ " . join(' â”‚ ', @{$def{'repr'}{$_}})." â”‚\n\n";
      } };
    }
}
# repr normal, ascii->ascii, any other keysym goes U
sub repr { # takes keysym hex, returns .skd representation string.
    my $in=shift; my $o;
    for($in) {
        /^0x1000$/       and return '  ';
        /^0x0000$/       and $o='â¥âƒ¥',last;
        /^(0x....)$/     and $o=$def{'sym'}{$1}[4] and last; # sname => outrepr, xxxx
        /^(0x....)$/     and $o=$def{'sym'}{$1}[1] and last; # sname => inrepr, xxxx
        /^0x00([0-7].)$/ and $o =chr(oct($_)),last; # ascii, in case of broken data
        /^0x(....)$/     and $o=$1,last;            # unknowns go by hex number
        /^0x1008ff(..)$/ and $o ="xF$1",last;       # XFree86 FF
        /^0x1008fe(..)$/ and $o ="xE$1",last;       # XFree86 FE
        /^0xffffffff$/   and $o='â¥',last;           # Void symbol
        /(0x10([012345]....))$/ and $o=$def{'uni'}{$1}[1] and last;
        #$2 or print("$_ unknown \$2!\n"), $o='*âƒœ', last;
        $2 or $o='*âƒœ', last; # unknown key
        $o=chr(oct("0x$2")); last;
        if(/(0x10([0123]....))$/){ # unicode
            my $cs="0x$2"; my $cp=oct($cs); my $c=chr($cp);
            $o=$def{'uni'}{$1}[1]||chr($cp); # chr(oct("0x$2"));
            last;
        } die "Should not happen!\n";
    }
    for($o){ # n:asis wide uwide Fn square circle .dead
      s/\s+//gaa;
      /^%[nwuFsc.]/ and return $o; # manual set
      /^\.(\X)/ and return "%.$1"; # deads
      /^\#?F(\d{1,2})/aa and return "%F$1"; # funcs
      /^\p{Mn}|^\p{Me}/ and return "â—Œ$o";
      /^\p{Mc}/        and return "%wâ—Œ$o";
      /^\p{Dt=Wide}|\p{Ea=W}/ and return "%w$o";
      index($W,$o)>=0 and return "%w$o";
      return $o;
    }
}
sub rdata {
  my ($c, $u, $k, $t);
  my $fh=shift||"main::DATA";
  while(<$fh>) {
    /^:\s/maa and next; # : comments allowed; double to escape a colon
    if(m'^:(\w):(\w+):\s'maa){
        $c and $def{$k}=$c; $t=$1; $k=$2;
        $t=~m'[sS]' and $c=$def{$k}||'', next;
        $t=~m'[UH]' and $c=$def{$k}||{}, next;
        $t=~m'[tT]' and $c=$def{$k}||[], next;
        $c=undef; print STDERR "Bad section :$t:$k: in line $.. Skipping.\n";
        next;
    } defined $c or next; /^::/ and s/^::/:/; # fix data line with escaped comment char
    if($t eq 'S'){ $c.=$_; next } chomp;
    if($t eq 'T'){ push @$c, $_; next; }
    if($t eq 'H'){ /^(\w+)\s/aa  and @{$c->{$1}}=split, shift @{$c->{$1}}; next; }
    if($t eq 'U'){ /^(0x[0-9a-f]+)\s/aa or next;
        my @e=split; $c->{$1}=\@e; $byU{$e[2]}=\@e; next }
    print STDERR "Type $t of :$t:$k: line $. is not supported!\n";
  }
  $k and $def{$k}=$c;
  close $fh;
}
sub mklayout { -d "$xkbdir/rules" or usage
    "Wrong XKB path: directory $xkbdir not found!\n"; }
__END__
